    "yargs": "^15.3.1",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/yargs": "^15.0.5",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "^14.0.5"
  }
}
