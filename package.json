{
  "name": "swagger-to-ts",
  "version": "0.0.3",
  "description": "Generate Typescript models and client interface from Swagger yaml",
  "author": "Eduardo Campver",
  "license": "ISC",
  "main": "build/index.js",
  "bin": {
    "swagger-to-ts": "build/index.js"
  },
  "scripts": {
    "build": "tsc --lib esnext --outDir ./build --downlevelIteration index.ts",
    "start": "node ./build/index.js",
